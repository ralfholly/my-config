xterm*faceName: DejaVuSansMono
xterm*faceSize: 10
xterm*utf8: 1
xterm*toolBar: false

xterm*foreground: #d5d5d5
xterm*background: #131313
!xterm*cursorColor: #ffc000
xterm*cursorColor: #00ff00

!xterm*foreground: black
!xterm*background: white
!xterm*cursorColor: #ffaf00

xterm*geometry: 120x48
xterm*cursorBlink: true
xterm*cursorOnTime: 500
xterm*cursorOffTime: 500

# XTerm*bellIsUrgent: true
# URxvt.urgentOnBell: true

XTerm.VT100.translations: #override \
Meta <Key> minus: smaller-vt-font() \n\
Meta <Key> plus: larger-vt-font() \n\

